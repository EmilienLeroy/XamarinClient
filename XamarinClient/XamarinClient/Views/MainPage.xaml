<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:XamarinClient.Views"
             x:Class="XamarinClient.Views.MainPage">

    <ListView x:Name="listeItineraires" ItemsSource="{Binding ListeDevises, Mode=TwoWay}" SeparatorVisibility="None">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Nom}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>

</ContentPage>
